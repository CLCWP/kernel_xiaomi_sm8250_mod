# Device Specific Board Configuration
CONFIG_BOARD_PIPA=y

# Airoha GPS
CONFIG_AIROHA_GPS=y

# Enable modified MSM Serial driver
# CONFIG_SERIAL_MSM_GENI is not set
CONFIG_SERIAL_MSM_GENI_ELISH=y

# Power supply drivers
# CONFIG_BQ2597X_CHARGE_PUMP is not set
# CONFIG_FUEL_GAUGE_BQ27Z561 is not set
# CONFIG_IDT_P9415 is not set
# CONFIG_LN8282 is not set
# CONFIG_QPNP_FG_GEN4 is not set
CONFIG_QPNP_FG_GEN4_PIPA=y
# CONFIG_QPNP_SMB5 is not set
# CONFIG_RX1619 is not set
# CONFIG_RX1619_TRIM is not set
CONFIG_BATT_VERIFY=y
CONFIG_BATT_VERIFY_BY_DS28E16_PIPA=y
CONFIG_BQ2597X_CHARGE_PUMP_PIPA=y
CONFIG_DUAL_BQ2597X=y
CONFIG_FUDA_1665=y
CONFIG_ONEWIRE_GPIO_PIPA=y
CONFIG_QPNP_SMB5_PIPA=y

# USB PD
# CONFIG_USB_PD_POLICY is not set
CONFIG_USB_PD_POLICY_DAGU=y
CONFIG_NO_PS_USB3=y
# CONFIG_PS5169 is not set

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Retry i2c operations on failure
CONFIG_I2C_RETRY=y

# Enable KTZ8866 backlight driver
CONFIG_BACKLIGHT_KTZ8866=y

# Disable ESOC
# CONFIG_ESOC is not set

# Enable NT36532 touchscreen driver
CONFIG_TOUCHSCREEN_NT36532_HOSTDL_SPI=y
CONFIG_TOUCHSCREEN_NVT_DEBUG_FS=y
CONFIG_TOUCHSCREEN_NEW_PEN_CONNECT_STRATEGY=y
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y

# Enable HIMAX HX83121 touchscreen driver
CONFIG_TOUCHSCREEN_HIMAX_CHIPSET=y
CONFIG_TOUCHSCREEN_HIMAX_IC_HX83121=y
CONFIG_TOUCHSCREEN_HIMAX_INSPECT=y
CONFIG_TOUCHSCREEN_HIMAX_DEBUG=y
CONFIG_TOUCHSCREEN_HIMAX_INCELL=y
# CONFIG_TOUCHSCREEN_HIMAX_ONCELL is not set
CONFIG_TOUCHSCREEN_HIMAX_COMMON=y

# Enable NANOSIC 8030 V2 keyboard
CONFIG_INPUT_KEYBOARD=y
CONFIG_KEYBOARD_NANO_8030_V2=y

# Enable mm-hardware detection
CONFIG_MMHARDWARE_OTHER_DETECTION=y

# Enable WACOM HID driver
CONFIG_HID_WACOM=y
